---
import type { HTMLAttributes } from "astro/types";

const { props } = Astro;

type Link = HTMLAttributes<"link">;
---

{
  props.extend.link?.map((attributes: Link) => (
    <link {...attributes} />
  ))
}
{
  props.extend.meta?.map(({ content, httpEquiv, name, property }) => (
    <meta
      content={content}
      http-equiv={httpEquiv}
      name={name}
      property={property}
    />
  ))
}
